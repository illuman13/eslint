{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends":
      ["airbnb"],
  "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "max-len": [1, 120, 2, {
        "ignoreComments": true
      }]
    }
}
